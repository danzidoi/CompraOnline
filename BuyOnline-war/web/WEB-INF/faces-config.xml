<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/logout.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>falha</from-outcome>
            <to-view-id>/falha.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
<!--    <managed-bean>  
        <managed-bean-name>loginMB</managed-bean-name>  
        <managed-bean-class>managedbeans.LoginMB</managed-bean-class>  
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>-->
    <managed-bean>
        <managed-bean-name>produtoWeb</managed-bean-name>
        <managed-bean-class>managedbeans.ProdutoWeb</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>posicaoCarrinhoWeb</managed-bean-name>
        <managed-bean-class>managedbeans.PosicaoCarrinhoWeb</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>clienteWeb</managed-bean-name>
        <managed-bean-class>managedbeans.ClienteWeb</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pedidoWeb</managed-bean-name>
        <managed-bean-class>managedbeans.PedidoWeb</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>


